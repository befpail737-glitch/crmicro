<!-- SVG chart for SiC comparison -->
<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <rect width="100%" height="100%" fill="#f9fafb"/>
  <text x="400" y="50" font-family="Arial, sans-serif" font-size="24" fill="#1f2937" text-anchor="middle">SiC vs Si Device Characteristics</text>
  
  <!-- Chart axes -->
  <line x1="100" y1="500" x2="700" y2="500" stroke="#1f2937" stroke-width="2"/>
  <line x1="100" y1="100" x2="100" y2="500" stroke="#1f2937" stroke-width="2"/>
  
  <!-- X axis labels -->
  <text x="200" y="530" font-family="Arial, sans-serif" font-size="14" fill="#1f2937" text-anchor="middle">Breakdown Voltage</text>
  <text x="350" y="530" font-family="Arial, sans-serif" font-size="14" fill="#1f2937" text-anchor="middle">Conduction Loss</text>
  <text x="500" y="530" font-family="Arial, sans-serif" font-size="14" fill="#1f2937" text-anchor="middle">Switching Loss</text>
  <text x="650" y="530" font-family="Arial, sans-serif" font-size="14" fill="#1f2937" text-anchor="middle">Thermal Cond.</text>
  
  <!-- SiC bars -->
  <rect x="170" y="300" width="60" height="200" fill="#1e40af" />
  <rect x="320" y="350" width="60" height="150" fill="#1e40af" />
  <rect x="470" y="400" width="60" height="100" fill="#1e40af" />
  <rect x="620" y="200" width="60" height="300" fill="#1e40af" />
  <text x="200" y="290" font-family="Arial, sans-serif" font-size="14" fill="#1e40af" text-anchor="middle">SiC</text>
  
  <!-- Si bars -->
  <rect x="240" y="200" width="60" height="300" fill="#0ea5e9" />
  <rect x="390" y="200" width="60" height="300" fill="#0ea5e9" />
  <rect x="540" y="250" width="60" height="250" fill="#0ea5e9" />
  <rect x="690" y="350" width="60" height="150" fill="#0ea5e9" />
  <text x="270" y="190" font-family="Arial, sans-serif" font-size="14" fill="#0ea5e9" text-anchor="middle">Si</text>
  
  <!-- Y axis labels -->
  <text x="70" y="505" font-family="Arial, sans-serif" font-size="12" fill="#1f2937" text-anchor="end">0</text>
  <text x="70" y="405" font-family="Arial, sans-serif" font-size="12" fill="#1f2937" text-anchor="end">25%</text>
  <text x="70" y="305" font-family="Arial, sans-serif" font-size="12" fill="#1f2937" text-anchor="end">50%</text>
  <text x="70" y="205" font-family="Arial, sans-serif" font-size="12" fill="#1f2937" text-anchor="end">75%</text>
  <text x="70" y="105" font-family="Arial, sans-serif" font-size="12" fill="#1f2937" text-anchor="end">100%</text>
  
  <!-- Legend -->
  <rect x="600" y="50" width="20" height="10" fill="#1e40af" />
  <text x="630" y="60" font-family="Arial, sans-serif" font-size="12" fill="#1f2937">Silicon Carbide</text>
  <rect x="600" y="70" width="20" height="10" fill="#0ea5e9" />
  <text x="630" y="80" font-family="Arial, sans-serif" font-size="12" fill="#1f2937">Silicon</text>
</svg>