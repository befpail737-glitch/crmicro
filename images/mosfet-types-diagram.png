<!-- MOSFET Symbol and Types -->
<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <rect width="100%" height="100%" fill="#f9fafb"/>
  <text x="400" y="40" font-family="Arial, sans-serif" font-size="24" fill="#1f2937" text-anchor="middle">CRMicro MOSFET Product Types</text>
  
  <!-- SGT MOSFET -->
  <g transform="translate(100, 150)">
    <rect width="180" height="200" fill="#dbeafe" stroke="#3b82f6" stroke-width="2" rx="10"/>
    <text x="90" y="30" font-family="Arial, sans-serif" font-size="16" fill="#1e3a8a" text-anchor="middle">SGT MOSFET</text>
    <text x="90" y="50" font-family="Arial, sans-serif" font-size="12" fill="#4b5563" text-anchor="middle">(Shielded Gate Trench)</text>
    
    <!-- MOSFET Symbol -->
    <rect x="50" y="70" width="80" height="80" fill="white" stroke="#1f2937" stroke-width="2"/>
    <line x1="30" y1="110" x2="50" y2="110" stroke="#1f2937" stroke-width="2"/>
    <line x1="130" y1="90" x2="150" y2="90" stroke="#1f2937" stroke-width="2"/>
    <line x1="130" y1="130" x2="150" y2="130" stroke="#1f2937" stroke-width="2"/>
    <text x="40" y="105" font-family="Arial, sans-serif" font-size="12" fill="#1f2937" text-anchor="end">G</text>
    <text x="160" y="85" font-family="Arial, sans-serif" font-size="12" fill="#1f2937" text-anchor="start">D</text>
    <text x="160" y="135" font-family="Arial, sans-serif" font-size="12" fill="#1f2937" text-anchor="start">S</text>
    
    <!-- Characteristics -->
    <text x="90" y="170" font-family="Arial, sans-serif" font-size="12" fill="#4b5563" text-anchor="middle">Low RDS(on)</text>
    <text x="90" y="185" font-family="Arial, sans-serif" font-size="12" fill="#4b5563" text-anchor="middle">Low Gate Charge</text>
    <text x="90" y="200" font-family="Arial, sans-serif" font-size="12" fill="#4b5563" text-anchor="middle">Low Voltage (20-100V)</text>
  </g>
  
  <!-- SJ MOSFET -->
  <g transform="translate(310, 150)">
    <rect width="180" height="200" fill="#d1fae5" stroke="#10b981" stroke-width="2" rx="10"/>
    <text x="90" y="30" font-family="Arial, sans-serif" font-size="16" fill="#065f46" text-anchor="middle">SJ MOSFET</text>
    <text x="90" y="50" font-family="Arial, sans-serif" font-size="12" fill="#4b5563" text-anchor="middle">(Super Junction)</text>
    
    <!-- MOSFET Symbol -->
    <rect x="50" y="70" width="80" height="80" fill="white" stroke="#1f2937" stroke-width="2"/>
    <line x1="30" y1="110" x2="50" y2="110" stroke="#1f2937" stroke-width="2"/>
    <line x1="130" y1="90" x2="150" y2="90" stroke="#1f2937" stroke-width="2"/>
    <line x1="130" y1="130" x2="150" y2="130" stroke="#1f2937" stroke-width="2"/>
    <text x="40" y="105" font-family="Arial, sans-serif" font-size="12" fill="#1f2937" text-anchor="end">G</text>
    <text x="160" y="85" font-family="Arial, sans-serif" font-size="12" fill="#1f2937" text-anchor="start">D</text>
    <text x="160" y="135" font-family="Arial, sans-serif" font-size="12" fill="#1f2937" text-anchor="start">S</text>
    
    <!-- Characteristics -->
    <text x="90" y="170" font-family="Arial, sans-serif" font-size="12" fill="#4b5563" text-anchor="middle">Low RDS(on)</text>
    <text x="90" y="185" font-family="Arial, sans-serif" font-size="12" fill="#4b5563" text-anchor="middle">Low Reverse Recovery</text>
    <text x="90" y="200" font-family="Arial, sans-serif" font-size="12" fill="#4b5563" text-anchor="middle">High Voltage (600-800V)</text>
  </g>
  
  <!-- Planar MOSFET -->
  <g transform="translate(520, 150)">
    <rect width="180" height="200" fill="#fef3c7" stroke="#f59e0b" stroke-width="2" rx="10"/>
    <text x="90" y="30" font-family="Arial, sans-serif" font-size="16" fill="#92400e" text-anchor="middle">Planar MOSFET</text>
    <text x="90" y="50" font-family="Arial, sans-serif" font-size="12" fill="#4b5563" text-anchor="middle">(Traditional Structure)</text>
    
    <!-- MOSFET Symbol -->
    <rect x="50" y="70" width="80" height="80" fill="white" stroke="#1f2937" stroke-width="2"/>
    <line x1="30" y1="110" x2="50" y2="110" stroke="#1f2937" stroke-width="2"/>
    <line x1="130" y1="90" x2="150" y2="90" stroke="#1f2937" stroke-width="2"/>
    <line x1="130" y1="130" x2="150" y2="130" stroke="#1f2937" stroke-width="2"/>
    <text x="40" y="105" font-family="Arial, sans-serif" font-size="12" fill="#1f2937" text-anchor="end">G</text>
    <text x="160" y="85" font-family="Arial, sans-serif" font-size="12" fill="#1f2937" text-anchor="start">D</text>
    <text x="160" y="135" font-family="Arial, sans-serif" font-size="12" fill="#1f2937" text-anchor="start">S</text>
    
    <!-- Characteristics -->
    <text x="90" y="170" font-family="Arial, sans-serif" font-size="12" fill="#4b5563" text-anchor="middle">High Reliability</text>
    <text x="90" y="185" font-family="Arial, sans-serif" font-size="12" fill="#4b5563" text-anchor="middle">Stable Characteristics</text>
    <text x="90" y="200" font-family="Arial, sans-serif" font-size="12" fill="#4b5563" text-anchor="middle">Wide Voltage Range</text>
  </g>
  
  <!-- Title box -->
  <rect x="50" y="500" width="700" height="80" fill="#f3f4f6" stroke="#d1d5db"/>
  <text x="400" y="530" font-family="Arial, sans-serif" font-size="14" fill="#4b5563" text-anchor="middle">CRMicro MOSFET Portfolio - Optimized for Different Applications</text>
  <text x="400" y="550" font-family="Arial, sans-serif" font-size="12" fill="#6b7280" text-anchor="middle">SGT: Low-voltage high-efficiency | SJ: High-voltage PFC & adapters | Planar: High-reliability automotive</text>
</svg>