<!-- Solar Inverter System Diagram -->
<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <rect width="100%" height="100%" fill="#f9fafb"/>
  <text x="400" y="40" font-family="Arial, sans-serif" font-size="24" fill="#1f2937" text-anchor="middle">Solar Inverter System - Power Conversion</text>
  
  <!-- Solar Panel -->
  <rect x="50" y="250" width="120" height="60" fill="#dbeafe" stroke="#3b82f6" stroke-width="2"/>
  <text x="110" y="285" font-family="Arial, sans-serif" font-size="14" fill="#1e3a8a" text-anchor="middle">Solar PV</text>
  <text x="110" y="305" font-family="Arial, sans-serif" font-size="12" fill="#4b5563" text-anchor="middle">DC Output ~30-60V</text>
  
  <!-- Arrow -->
  <line x1="170" y1="280" x2="230" y2="280" stroke="#1f2937" stroke-width="2" marker-end="url(#arrowhead)"/>
  
  <!-- DC-DC Boost -->
  <rect x="230" y="250" width="120" height="60" fill="#dcfce7" stroke="#22c55e" stroke-width="2"/>
  <text x="290" y="285" font-family="Arial, sans-serif" font-size="14" fill="#166534" text-anchor="middle">DC-DC Boost</text>
  <text x="290" y="305" font-family="Arial, sans-serif" font-size="12" fill="#4b5563" text-anchor="middle">CRMicro SiC MOSFET</text>
  
  <!-- Arrow -->
  <line x1="350" y1="280" x2="410" y2="280" stroke="#1f2937" stroke-width="2" marker-end="url(#arrowhead)"/>
  
  <!-- DC Bus -->
  <rect x="410" y="250" width="120" height="60" fill="#fffbeb" stroke="#f59e0b" stroke-width="2"/>
  <text x="470" y="285" font-family="Arial, sans-serif" font-size="14" fill="#92400e" text-anchor="middle">DC Bus</text>
  <text x="470" y="305" font-family="Arial, sans-serif" font-size="12" fill="#4b5563" text-anchor="middle">400V</text>
  
  <!-- Arrow -->
  <line x1="530" y1="280" x2="590" y2="280" stroke="#1f2937" stroke-width="2" marker-end="url(#arrowhead)"/>
  
  <!-- Inverter -->
  <rect x="590" y="250" width="120" height="60" fill="#ffe4e6" stroke="#e11d48" stroke-width="2"/>
  <text x="650" y="285" font-family="Arial, sans-serif" font-size="14" fill="#991b1b" text-anchor="middle">Grid-Tie Inv</text>
  <text x="650" y="305" font-family="Arial, sans-serif" font-size="12" fill="#4b5563" text-anchor="middle">CRMicro SiC MOSFET</text>
  
  <!-- Grid -->
  <rect x="730" y="250" width="120" height="60" fill="#ddd6fe" stroke="#8b5cf6" stroke-width="2"/>
  <text x="790" y="285" font-family="Arial, sans-serif" font-size="14" fill="#5b21b6" text-anchor="middle">Grid</text>
  <text x="790" y="305" font-family="Arial, sans-serif" font-size="12" fill="#4b5563" text-anchor="middle">AC 230V/400V</text>
  
  <!-- MPPT Controller -->
  <rect x="300" y="80" width="200" height="60" fill="#bbf7d0" stroke="#15803d" stroke-width="2"/>
  <text x="400" y="110" font-family="Arial, sans-serif" font-size="14" fill="#166534" text-anchor="middle">MPPT Algorithm</text>
  <text x="400" y="130" font-family="Arial, sans-serif" font-size="12" fill="#4b5563" text-anchor="middle">CRMicro MCU & ADC</text>
  
  <!-- Control lines -->
  <line x1="400" y1="140" x2="400" y2="250" stroke="#1f2937" stroke-width="1" stroke-dasharray="5,5"/>
  <line x1="400" y1="140" x2="300" y2="280" stroke="#1f2937" stroke-width="1" stroke-dasharray="5,5"/>
  <line x1="400" y1="140" x2="500" y2="280" stroke="#1f2937" stroke-width="1" stroke-dasharray="5,5"/>
  
  <!-- Monitoring & Protection -->
  <circle cx="450" cy="200" r="25" fill="#ddd6fe" stroke="#8b5cf6" stroke-width="2"/>
  <text x="450" y="205" font-family="Arial, sans-serif" font-size="10" fill="#5b21b6" text-anchor="middle">Protection &</text>
  <text x="450" y="218" font-family="Arial, sans-serif" font-size="10" fill="#5b21b6" text-anchor="middle">Monitoring</text>
  <line x1="450" y1="225" x2="460" y2="250" stroke="#1f2937" stroke-width="1" stroke-dasharray="5,5"/>
  
  <!-- Definitions for arrows -->
  <defs>
    <marker id="arrowhead" markerWidth="10" markerHeight="7" refX="10" refY="3.5" orient="auto">
      <polygon points="0 0, 10 3.5, 0 7" fill="#1f2937"/>
    </marker>
  </defs>
  
  <!-- Title box -->
  <rect x="50" y="530" width="700" height="50" fill="#f3f4f6" stroke="#d1d5db"/>
  <text x="400" y="560" font-family="Arial, sans-serif" font-size="14" fill="#4b5563" text-anchor="middle">Solar String Inverter - CRMicro SiC Technology for High Efficiency</text>
</svg>